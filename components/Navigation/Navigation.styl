.navigationBar
    min-width: 185px
    width: 15%
    display: flex
    flex-direction: column
    align-items: flex-end
    flex-shrink: 0
    padding-right: 5px;
    background-image: linear-gradient(to bottom, #00979d 0%, #00979d 100%), linear-gradient(to bottom, #fff 0%, #fff 100%);
    background-clip: content-box, padding-box;
    border-right: 5px solid #aaa

.navigationBar::before
    content: ''
    height: 200px
    display: block