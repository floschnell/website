.title
    font-size: 16px
    font-weight: bold
    margin-top: 2em
    margin-bottom: 1em

.list
    margin: 0
    padding: 0
    list-style-type: none

.item
    font-size: 14px
    padding-left: 1em
    line-height: 30px
    a
        color: black
        text-decoration: none
    a:hover
        text-decoration: underline
    :global(.material-icons)
        vertical-align: middle
        margin-right: 5px